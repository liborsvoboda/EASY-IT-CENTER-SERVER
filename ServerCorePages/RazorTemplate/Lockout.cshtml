@page "/RazorTemplate/Lockout"
@{
    Layout = "/ServerCorePages/Shared/RazorTemplateLayout.cshtml";
}

@model ServerCorePages.LockoutModel
@{
    ViewData["Title"] = "Locked out";
}

<div class="login-box-body">
    <p class="login-box-msg text-danger">
        This account has been locked out,<br /> please try again later.
    </p>
    <p class="login-box-msg">
        <a asp-page="/RazorTemplate/Login">Click here to log in again</a>.
    </p>
</div>
<!-- /.login-box-body -->

